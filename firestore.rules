rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /users/{userId} {
      allow get: if true;
      allow list: if request.auth.uid != null;
      allow write: if request.auth.uid == userId;
    }

    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2021, 3, 17);
    }
  }
}